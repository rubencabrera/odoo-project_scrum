<?xml version="1.0"?>
<openerp>
    <data noupdate="1">

        <record id="project_marketing" model="project.project">
            <field name="date_start" eval="time.strftime('%Y-%m-01 10:00:00')"/>
            <field name="state">open</field>
            <field name="type">normal</field>
            <field name="name">Marketing Project</field>
            <field name="color">3</field>
            <field name="parent_id" ref="project.all_projects_account"/>
            <field name="privacy_visibility">public</field>
            <field name="description">Implement Marketing project.</field>
            <field name="alias_model">project.task</field>
        </record>

        <record id="project_facebook" model="project.project">
            <field name="date_start" eval="time.strftime('%Y-%m-01 10:00:00')"/>
            <field name="state">open</field>
            <field name="type">normal</field>
            <field name="name">Facebook Campaigns</field>
            <field name="color">3</field>
            <field name="parent_id" ref="project.all_projects_account"/>
            <field name="privacy_visibility">public</field>
            <field name="description">Implement Facebook project.</field>
            <field name="alias_model">project.task</field>
        </record>



    </data>
</openerp>
